
<header>

  <nav class="navbar navbar-expand-md navbar-dark bg-dark">
    <a class="navbar-brand" href="/">TinyApp</a>

    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      
      <div class="navbar-nav">
        <a class="nav-item navbar-brand" style="margin-left: 25px" href="/urls">My URLs</a>
        <a class="nav-item navbar-brand" style="margin-left: 25px;" href="/urls/new">Create New URL</a>

      </div>
    </div>
        <% if (userObject) { %>
              <h6 class="nav-item nav-link text-light" style="margin-right: 10px; margin-top: 10px;">Hello <%= userObject.email %></h6>
              
              <form method="POST" action="/logout">
                <button class="btn btn-primary">Logout</button>
              </form>
        <% } else { %>
              <button class="btn btn-primary" style="margin-right: 10px; padding-left: 28px; padding-right: 28px;" onclick="window.location.href='/login';">Login</button>
              <button class="btn btn-primary" style="padding-left: 20px; padding-right: 20px;" onclick="window.location.href='/register';">Register</button>
        <% } %>

  </nav>
</header>
